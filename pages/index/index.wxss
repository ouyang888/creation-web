}

.loginBtn {
  margin: 0 auto;
  color: #fff;
  background: #00c237;
  height: 88rpx;
  font-size: 32rpx;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
}

button:not([size='mini']) {
